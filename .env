# LLM Provider Configuration
# Choose either "openai" or "gemini"
LJS_LLM_PROVIDER=gemini

# Model Selection (adjust based on provider)
# For OpenAI: gpt-4-turbo-preview, gpt-4, gpt-3.5-turbo
# For Gemini: gemini-1.5-pro, gemini-1.5-flash
LJS_LLM_MODEL=gemini-1.5-flash

# API Keys (set the one you're using)
OPENAI_API_KEY=
GOOGLE_API_KEY=

# Optional: Timeout and token limits
LJS_LLM_TIMEOUT=60
LJS_LLM_MAX_TOKENS=4000